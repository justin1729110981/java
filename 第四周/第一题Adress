package adress;

public class Adress {
	private String country;
	private String province;
	private String city;
	private String street;
	private int number;
	
	public Adress(String country,String province,String city,String street,int number) {
		this.country=country;
		this.province=province;
		this.city=city;
		this.street=street;
		this.number=number;
	}
	public void display() {
		String info="";
		info=info+this.country+this.province+this.city+this.street+this.number;
		System.out.println(info);
	}
}
