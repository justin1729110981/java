package homework1.vo;

public class StudentResult extends Student{
	private float Avergrade;

	public StudentResult() {
		super();
	}

	public StudentResult(String id, String name, String gender, float avergrade) {
		super(id, name, gender);
		Avergrade = avergrade;
	}
	
	public float getAvergrade() {
		return Avergrade;
	}

	public void setAvergrade(float avergrade) {
		Avergrade = avergrade;
	}

	@Override
	public String toString() {
		return super.toString()+"\t"+ Avergrade;
	}
	
}
